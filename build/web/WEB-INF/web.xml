<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>deletestudent</servlet-name>
        <servlet-class>deletestudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewstudent</servlet-name>
        <servlet-class>viewstudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServ</servlet-name>
        <servlet-class>NewServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addquestion</servlet-name>
        <servlet-class>Addquestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deletequestion</servlet-name>
        <servlet-class>Deletequestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Viewquestions</servlet-name>
        <servlet-class>Viewquestions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addteacher</servlet-name>
        <servlet-class>Addteacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreMessages</servlet-name>
        <servlet-class>StoreMessages</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deletestudent</servlet-name>
        <url-pattern>/deletestudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewstudent</servlet-name>
        <url-pattern>/viewstudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServ</servlet-name>
        <url-pattern>/NewServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addquestion</servlet-name>
        <url-pattern>/Addquestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deletequestion</servlet-name>
        <url-pattern>/Deletequestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Viewquestions</servlet-name>
        <url-pattern>/Viewquestions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addteacher</servlet-name>
        <url-pattern>/Addteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreMessages</servlet-name>
        <url-pattern>/StoreMessages</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
